{
  "name": "Codeforces helper",
  "version": "0.1",
  "description": "Help you download test cases, more features TBD",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/contest/*/problem/*",
        "https://codeforces.com/problemset/problem/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "manifest_version": 2
}